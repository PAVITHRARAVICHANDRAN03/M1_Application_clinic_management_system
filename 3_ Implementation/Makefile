# Name of the project
PROJECT_NAME = Clinic Management System

# All source code files
SRC = clinic_management_system.c\
src/clinic main.c
OBJ = clinic main.o
INC = inc
CFLAGS = I$(INC)
a.out:$(OBJ)
   gcc $ -I$(INC)-o $@
%.o:src%
   gcc $^ -I$(INC)-c
run : target
   ./all.out
clear:
   rm all.out *.o src/*.o
   

